{
    "name": "@buvarp/csv-to-xlsx",
    "description": "JavaScript library for converting CSV files to XLSX",
    "version": "0.0.1",
    "type": "commonjs",
    "main": "./lib/index.js",
    "types": "./lib/index.d.ts",
    "license": "MIT",
    "keywords": [
        "csv",
        "xlsx",
        "convert",
        "converter"
    ],
    "author": "Magnus Buvarp <magnus.buvarp@gmail.com>",
    "publishConfig": {
        "access": "public"
    },
    "scripts": {
        "start": "node ./bin/index.js",
        "dev": "ts-node ./src/index.ts",
        "build": "tsc",
        "prepare": "husky install .husky"
    },
    "dependencies": {
        "csv-parse": "^5.3.0",
        "xlsx": "^0.18.5"
    },
    "devDependencies": {
        "@types/node": "^18.6.4",
        "husky": "7.0.2",
        "prettier": "^2.7.1",
        "pretty-quick": "^3.1.3",
        "typescript": "^4.7.4"
    }
}
